<ModuleCfgXml>
  <name>ZLAdapt</name>
  <!-- id is the base info, usr can assign or use default, it must be multi of 100, the value smaller than is 10000 is reserved for system -->>
  <!-- name of the class, libname+namespace+classname -->
  <cls>WinApp.hw.zl.Zladapt</cls>
  <version>0.1</version>
  <init>4</init>
  <ipaddr>127.0.0.1</ipaddr>

  <usertasks>
    <name>ZLHWTcp</name>
  </usertasks>

  <faninc>
    <fanin>
      <point datafmt="byte">
      <name>ZLAdapt.GetZLHW</name>
        </point>
    </fanin>
  </faninc>

  <faninfanouts>
    <faninfanout>
      <fanin>
        <point datafmt="byte">
        <name>GetZLHW</name>
          </point>
      </fanin>
      <fanout>
        <point datafmt="byte">
        <name>ZLHWFanOut</name>
          </point>
      </fanout>
    </faninfanout>
  </faninfanouts>

  <fanworks>
    <fanwork>
      <workernum>1</workernum>
      <fanin>
        <point>
        <name>Device.HwData</name>
          </point>
      </fanin>
      <fanout datafmt="byte">
        <point>
        <name>ZLAdapt.ZLHWFanOut</name>
          </point>
      </fanout>
    </fanwork>
  </fanworks>


</ModuleCfgXml>