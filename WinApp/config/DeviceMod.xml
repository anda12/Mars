<ModuleCfgXml>
  <name>Device</name>
  <!-- id is the base info, usr can assign or use default, it must be multi of 100, the value smaller than is 10000 is reserved for system -->>
  <cls>WinApp.hw.Device</cls>
  <version>0.1</version>
  <init>3</init>
  <ipaddr>127.0.0.1</ipaddr>

  <depends>
    <depend>Warning</depend>
  </depends>
  
  <faninc>
    <fanin>
      <point>
        <name>Device.HwData</name>
      </point>
    </fanin>
    <fanin>
      <point>
        <name>Warning.WarnData</name>
      </point>
    </fanin>
  </faninc>

  <subs>
    <sub>
      <point>
        <name>Device.HwDataPub</name>
      </point>
    </sub>
  </subs>
  
  <requests>
    <request>
      <point>
        <name>Warning.SysWarn</name>
      </point>
    </request>
  </requests>

  <faninpubs>
    <faninpub>
      <fanin>
        <point>
          <name>HwData</name>
        </point>
      </fanin>
      <pub>
        <point>
          <name>HwDataPub</name>
        </point>
      </pub>
    </faninpub>
  </faninpubs>

  
  <responses>
    <point>
      <name>DevService</name>
    </point>
  </responses>

</ModuleCfgXml>